#pragma warning (disable : 4996)
#include <stdio.h>
int main() 
{
	int n,i=0,sum=0;
	while (sum<1000)
	{
		sum= sum + i++;
	}
	n = i-1 ;
	printf("n의 최댓값은 %d", n);
	return 0 ;
}
{
	char fru;
	printf("영문자를 입력해주세요:");
	scanf("%c", &fru);

	switch (fru)
	{
	case 'A': printf("");   //정수,문자형 상수만 case 뒤 값으로 옴
	default: printf("아직 해당 대문자에 문자가 할당되지 않아요") ;
	}

	return 0;
}
------------------------------------
#pragma warning (disable : 4996)
#include <stdio.h>
int main() //1~100까지 수 중에서 3배수만 출력하는 프로그램 작성
{
	int num;
	
	num = 1;
	while (num<=100)  //초기값****
	{
		if (num%3==0)
		printf("%d",num);
		num ++;
		
	}

	return 0;
}
-------------------------------------------
#pragma warning (disable : 4996)
#include <stdio.h>
int main() //1~100까지 수 중에서 3배수 개수 출력하는 프로그램 작성
{
	int num,au;
	
	num = 1;
	au = 0;
	while (num<=100)
	{
		if (num % 3 == 0)
			au++;
		num ++;

		
	}
	printf("개수는 %d", au);
	return 0;
}


